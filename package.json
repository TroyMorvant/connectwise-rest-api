{
  "name": "connectwise-rest-api",
  "version": "0.0.11",
  "main": "./release/connectwise.js",
  "typings": "./connectwise.d.ts",
  "description": "Nodejs Module to interact with Connectwise's REST API",
  "author": {
    "name": "Cameron Sowder",
    "email": "cameron@sowder.io"
  },
  "xo": {
    "esnext": true,
    "space": false,
    "ignore": [
      "packages/**",
      "build/**",
      "release/**"
    ],
    "rules": {
      "no-undef": 0,
      "indent": 0,
      "max-len": [
        2,
        200,
        4,
        {
          "ignoreComments": true,
          "ignoreUrls": true,
          "ignorePattern": "^\\s*var\\s.+=\\s*require\\s*\\("
        }
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "brace-style": [
        2,
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ],
      "babel/object-curly-spacing": 0,
      "prefer-arrow-callback": 0,
      "babel/object-shorthand": 0,
      "object-shorthand": 0,
      "no-labels": 0,
      "no-empty-label": 0,
      "no-native-reassign": 0
    }
  },
  "private": false,
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-runtime": "^6.9.0",
    "chalk": "^1.1.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-plumber": "^1.1.0",
    "gulp-size": "^2.1.0",
    "gulp-tslint": "^5.0.0",
    "gulp-tslint-stylish": "^1.1.1",
    "gulp-typescript": "^2.13.5",
    "gulp-uglify": "^1.5.3",
    "merge2": "^1.0.2",
    "pretty-error": "^2.0.0",
    "rimraf": "^2.5.2",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "xo": "^0.15.1"
  },
  "readme": "ERROR: No README data found!",
  "_id": "connectwise-rest-api@0.0.4",
  "scripts": {
    "test": "xo && ava"
  }
}
